olbar-title>Select an Avatar</v-toolbar-title>\n                <v-spacer></v-spacer>\n            </v-toolbar>\n            <v-layout row wrap v-if=\"avatars\">\n                <v-flex\n                    v-for=\"avatar in avatars\"\n                    :key=\"avatar.id\"\n                    xs4 sm3\n                    d-flex>\n                    <v-card tile flat class=\"d-flex\">\n                        <v-card-text class=\"d-flex\">\n                            <v-avatar\n                                size=\"96\"\n                                @click=\"selectAvatar(avatar)\"\n                                class=\"avatar-picker-avatar\"\n                                :class=\"{ 'current': avatar.id === currentAvatar }\">\n                                <img :src=\"'/avatars/' + (avatar.path)\">\n                            </v-avatar>\n                        </v-card-text>\n                    </v-card>\n                </v-flex>\n            </v-layout>\n        </v-card>\n    </v-dialog>\n</template>\n\n<script>\n    export default {\n        props: {\n            currentAvatar: {\n                type: String,\n                required: true\n            },\n            value: Boolean\n        },\n        async mounted () {\n            await this.$store.dispatch('fetchAvatars')\n        },\n        computed: {\n            avatars () {\n                return this.$store.state.avatars\n            },\n            show: {\n                get () {\n                    return this.value\n                },\n                set (value) {\n                    this.$emit('input', value)\n                }\n            }\n        },\n        methods: {\n            selectAvatar (avatar) {\n                this.$emit('selected', avatar.id)\n                this.show = false\n            }\n        }\n    }\n</script>"],"sourceRoot":"src/components"}]}